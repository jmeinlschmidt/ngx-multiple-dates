{
  "name": "ngx-multiple-dates",
  "description": "Angular Multiple Dates",
  "version": "1.0.0-beta.0",
  "homepage": "https://lekhmanrus.github.io/ngx-multiple-dates/",
  "bugs": {
    "url": "https://github.com/lekhmanrus/ngx-multiple-dates/issues",
    "email": "lekhman112@gmail.com"
  },
  "author": "Ruslan Lekhman <lekhman112@gmail.com> (https://github.com/lekhmanrus)",
  "contributors": [
    "Ruslan Lekhman <lekhman112@gmail.com> (https://github.com/lekhmanrus)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lekhmanrus/ngx-multiple-dates"
  },
  "license": "MIT",
  "readme": "https://github.com/lekhmanrus/ngx-multiple-dates/blob/master/README.md",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "postbuild": "npm-run-all --parallel copy:docs scss-bundle css",
    "copy:docs": "cpx \"*.md\" \"dist/ngx-multiple-dates\"",
    "scss-bundle": "scss-bundle",
    "css": "npm-run-all css:compile css:prefix css:minify",
    "css:compile": "npm-run-all --parallel css:compile:deeppurple-amber css:compile:indigo-pink css:compile:pink-bluegrey css:compile:purple-green",
    "css:compile:deeppurple-amber": "node-sass --output-style=expanded --precision=6 --importer=node_modules/node-sass-package-importer/dist/cli.js ./projects/ngx-multiple-dates/src/prebuilt-themes/deeppurple-amber.scss -o ./dist/ngx-multiple-dates/prebuilt-themes/",
    "css:compile:indigo-pink": "node-sass --output-style=expanded --precision=6 --importer=node_modules/node-sass-package-importer/dist/cli.js ./projects/ngx-multiple-dates/src/prebuilt-themes/indigo-pink.scss -o ./dist/ngx-multiple-dates/prebuilt-themes/",
    "css:compile:pink-bluegrey": "node-sass --output-style=expanded --precision=6 --importer=node_modules/node-sass-package-importer/dist/cli.js ./projects/ngx-multiple-dates/src/prebuilt-themes/pink-bluegrey.scss -o ./dist/ngx-multiple-dates/prebuilt-themes/",
    "css:compile:purple-green": "node-sass --output-style=expanded --precision=6 --importer=node_modules/node-sass-package-importer/dist/cli.js ./projects/ngx-multiple-dates/src/prebuilt-themes/purple-green.scss -o ./dist/ngx-multiple-dates/prebuilt-themes/",
    "css:minify": "npm-run-all --parallel css:minify:deeppurple-amber css:minify:indigo-pink css:minify:pink-bluegrey css:minify:purple-green",
    "css:minify:deeppurple-amber": "cleancss --level=1 --format=breakWith=lf ./dist/ngx-multiple-dates/prebuilt-themes/deeppurple-amber.css -o ./dist/ngx-multiple-dates/prebuilt-themes/deeppurple-amber.css",
    "css:minify:indigo-pink": "cleancss --level=1 --format=breakWith=lf ./dist/ngx-multiple-dates/prebuilt-themes/indigo-pink.css -o ./dist/ngx-multiple-dates/prebuilt-themes/indigo-pink.css",
    "css:minify:pink-bluegrey": "cleancss --level=1 --format=breakWith=lf ./dist/ngx-multiple-dates/prebuilt-themes/pink-bluegrey.css -o ./dist/ngx-multiple-dates/prebuilt-themes/pink-bluegrey.css",
    "css:minify:purple-green": "cleancss --level=1 --format=breakWith=lf ./dist/ngx-multiple-dates/prebuilt-themes/purple-green.css -o ./dist/ngx-multiple-dates/prebuilt-themes/purple-green.css",
    "css:prefix": "postcss --replace \"./dist/ngx-multiple-dates/prebuilt-themes/*.css\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": false,
  "keywords": [
    "angular",
    "material",
    "date",
    "multiple",
    "datepicker"
  ],
  "dependencies": {
    "@angular/animations": "9.0.0-rc.11",
    "@angular/cdk": "9.0.0-rc.8",
    "@angular/common": "9.0.0-rc.11",
    "@angular/compiler": "9.0.0-rc.11",
    "@angular/core": "9.0.0-rc.11",
    "@angular/forms": "9.0.0-rc.11",
    "@angular/material": "9.0.0-rc.8",
    "@angular/platform-browser": "9.0.0-rc.11",
    "@angular/platform-browser-dynamic": "9.0.0-rc.11",
    "@angular/router": "9.0.0-rc.11",
    "@material-extended/mde": "2.3.1",
    "hammerjs": "2.0.8",
    "rxjs": "6.5.4",
    "tslib": "1.10.0",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.900.0-rc.10",
    "@angular-devkit/build-ng-packagr": "0.900.0-rc.10",
    "@angular/cli": "9.0.0-rc.10",
    "@angular/compiler-cli": "9.0.0-rc.11",
    "@angular/language-service": "9.0.0-rc.11",
    "@types/jasmine": "3.5.1",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "13.5.0",
    "clean-css-cli": "4.3.0",
    "codelyzer": "5.2.1",
    "cpx": "1.5.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "3.1.0",
    "karma-jasmine-html-reporter": "1.5.1",
    "ng-packagr": "9.0.0-rc.7",
    "node-sass": "4.13.1",
    "node-sass-package-importer": "5.3.2",
    "npm-run-all": "4.1.5",
    "postcss-cli": "7.1.0",
    "postcss-load-config": "2.1.0",
    "protractor": "6.0.0",
    "scss-bundle": "3.0.2",
    "ts-node": "8.6.2",
    "tslint": "6.0.0",
    "typescript": "3.7.5"
  }
}
