{
  "name": "ngx-multiple-dates",
  "description": "Angular Multiple Dates",
  "version": "12.0.0",
  "homepage": "https://lekhmanrus.github.io/ngx-multiple-dates/",
  "bugs": {
    "url": "https://github.com/lekhmanrus/ngx-multiple-dates/issues",
    "email": "lekhman112@gmail.com"
  },
  "author": "Ruslan Lekhman <lekhman112@gmail.com> (https://github.com/lekhmanrus)",
  "contributors": [
    "Ruslan Lekhman <lekhman112@gmail.com> (https://github.com/lekhmanrus)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lekhmanrus/ngx-multiple-dates"
  },
  "license": "MIT",
  "readme": "https://github.com/lekhmanrus/ngx-multiple-dates/blob/master/README.md",
  "scripts": {
    "prepare": "is-ci || husky install",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "postbuild": "run-p copy:docs copy:scss css",
    "build:app": "ng build ngx-multiple-dates-app --base-href=/ngx-multiple-dates/",
    "postbuild:app": "cpx \"{LICENSE,CHANGELOG.md,README.md}\" \"dist/ngx-multiple-dates-app\"",
    "copy:docs": "cpx \"*.md\" \"dist/ngx-multiple-dates\"",
    "copy:scss": "cpx \"projects/ngx-multiple-dates/src/lib/**/*.scss\" \"dist/ngx-multiple-dates\"",
    "css": "run-s css:compile css:prefix css:minify",
    "css:compile": "run-p css:compile:deeppurple-amber css:compile:indigo-pink css:compile:pink-bluegrey css:compile:purple-green",
    "css:compile:deeppurple-amber": "sass -I node_modules ./projects/ngx-multiple-dates/src/prebuilt-themes/deeppurple-amber.scss:./dist/ngx-multiple-dates/prebuilt-themes/deeppurple-amber.css",
    "css:compile:indigo-pink": "sass -I node_modules ./projects/ngx-multiple-dates/src/prebuilt-themes/indigo-pink.scss:./dist/ngx-multiple-dates/prebuilt-themes/indigo-pink.css",
    "css:compile:pink-bluegrey": "sass -I node_modules ./projects/ngx-multiple-dates/src/prebuilt-themes/pink-bluegrey.scss:./dist/ngx-multiple-dates/prebuilt-themes/pink-bluegrey.css",
    "css:compile:purple-green": "sass -I node_modules ./projects/ngx-multiple-dates/src/prebuilt-themes/purple-green.scss:./dist/ngx-multiple-dates/prebuilt-themes/purple-green.css",
    "css:minify": "run-p css:minify:deeppurple-amber css:minify:indigo-pink css:minify:pink-bluegrey css:minify:purple-green",
    "css:minify:deeppurple-amber": "cleancss -f breakWith=lf ./dist/ngx-multiple-dates/prebuilt-themes/deeppurple-amber.css -o ./dist/ngx-multiple-dates/prebuilt-themes/deeppurple-amber.css",
    "css:minify:indigo-pink": "cleancss -f breakWith=lf ./dist/ngx-multiple-dates/prebuilt-themes/indigo-pink.css -o ./dist/ngx-multiple-dates/prebuilt-themes/indigo-pink.css",
    "css:minify:pink-bluegrey": "cleancss -f breakWith=lf ./dist/ngx-multiple-dates/prebuilt-themes/pink-bluegrey.css -o ./dist/ngx-multiple-dates/prebuilt-themes/pink-bluegrey.css",
    "css:minify:purple-green": "cleancss -f breakWith=lf ./dist/ngx-multiple-dates/prebuilt-themes/purple-green.css -o ./dist/ngx-multiple-dates/prebuilt-themes/purple-green.css",
    "css:prefix": "postcss --replace \"./dist/ngx-multiple-dates/prebuilt-themes/*.css\"",
    "test": "ng test",
    "test:ci": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release": "standard-version",
    "commitlint": "commitlint"
  },
  "private": false,
  "keywords": [
    "angular",
    "material",
    "date",
    "multiple",
    "datepicker"
  ],
  "dependencies": {
    "@angular/animations": "^13.0.3",
    "@angular/cdk": "^13.1.0",
    "@angular/common": "^13.0.3",
    "@angular/compiler": "^13.0.3",
    "@angular/core": "^13.0.3",
    "@angular/forms": "^13.0.3",
    "@angular/material": "^13.1.0",
    "@angular/platform-browser": "^13.0.3",
    "@angular/platform-browser-dynamic": "^13.0.3",
    "@angular/router": "^13.0.3",
    "@material-extended/mde": "^3.0.3",
    "rxjs": "^7.4.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.0.4",
    "@angular-eslint/builder": "^13.0.2-alpha.0",
    "@angular-eslint/eslint-plugin": "^13.0.2-alpha.0",
    "@angular-eslint/eslint-plugin-template": "^13.0.2-alpha.0",
    "@angular-eslint/schematics": "^13.0.2-alpha.0",
    "@angular-eslint/template-parser": "^13.0.2-alpha.0",
    "@angular/cli": "^13.0.4",
    "@angular/compiler-cli": "^13.0.3",
    "@angular/language-service": "^13.0.3",
    "@commitlint/cli": ">=15.0.0",
    "@commitlint/config-conventional": ">=15.0.0",
    "@commitlint/format": ">=15.0.0",
    "@types/jasmine": ">=3.10.2",
    "@types/jasminewd2": ">=2.0.10",
    "@types/karma-coverage": ">=2.0.1",
    "@types/karma-jasmine-html-reporter": ">=1.7.0",
    "@types/node": ">=16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.6.1-alpha.13",
    "@typescript-eslint/parser": "^5.6.1-alpha.13",
    "autoprefixer": "^10.4.0",
    "clean-css-cli": "5.5.0",
    "codelyzer": "^6.0.2",
    "conventional-changelog-angular": ">=5.0.13",
    "cpx": "^1.5.0",
    "eslint": "^8.4.1",
    "eslint-import-resolver-typescript": ">=2.5.0",
    "eslint-plugin-import": ">=2.25.3",
    "eslint-plugin-jsdoc": ">=37.1.0",
    "eslint-plugin-prefer-arrow": ">=1.2.3",
    "eslint-plugin-rxjs": ">=4.0.3",
    "eslint-plugin-unicorn": ">=39.0.0",
    "husky": "^7.0.4",
    "is-ci": ">=3.0.1",
    "jasmine-core": ">=3.10.1",
    "jasmine-spec-reporter": ">=7.0.0",
    "karma": ">=6.3.9",
    "karma-chrome-launcher": ">=3.1.0",
    "karma-coverage": ">=2.1.0",
    "karma-jasmine": ">=4.0.1",
    "karma-jasmine-html-reporter": ">=1.7.0",
    "karma-mocha-reporter": ">=2.2.5",
    "ng-packagr": "^13.1.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.0.2",
    "postcss-load-config": "^3.1.0",
    "protractor": "^7.0.0",
    "sass": "^1.45.0-rc.2",
    "standard-version": ">=9.3.2",
    "ts-node": "^10.4.0",
    "typescript": "~4.4.3"
  }
}
