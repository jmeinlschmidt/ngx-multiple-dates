{
  "name": "ngx-multiple-dates",
  "description": "Angular Multiple Dates",
  "version": "1.0.0",
  "homepage": "https://lekhmanrus.github.io/ngx-multiple-dates/",
  "bugs": {
    "url": "https://github.com/lekhmanrus/ngx-multiple-dates/issues",
    "email": "lekhman112@gmail.com"
  },
  "author": "Ruslan Lekhman <lekhman112@gmail.com> (https://github.com/lekhmanrus)",
  "contributors": [
    "Ruslan Lekhman <lekhman112@gmail.com> (https://github.com/lekhmanrus)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lekhmanrus/ngx-multiple-dates"
  },
  "license": "MIT",
  "readme": "https://github.com/lekhmanrus/ngx-multiple-dates/blob/master/README.md",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "postbuild": "run-p copy:docs scss-bundle css",
    "build:app": "ng build ngx-multiple-dates-app --prod --base-href=/ngx-multiple-dates/",
    "copy:docs": "cpx \"*.md\" \"dist/ngx-multiple-dates\"",
    "scss-bundle": "scss-bundle -c ./ -p ./",
    "css": "run-s css:compile css:prefix css:minify",
    "css:compile": "run-p css:compile:deeppurple-amber css:compile:indigo-pink css:compile:pink-bluegrey css:compile:purple-green",
    "css:compile:deeppurple-amber": "node-sass --output-style=expanded --precision=6 --importer=node_modules/node-sass-package-importer/dist/cli.js ./projects/ngx-multiple-dates/src/prebuilt-themes/deeppurple-amber.scss -o ./dist/ngx-multiple-dates/prebuilt-themes/",
    "css:compile:indigo-pink": "node-sass --output-style=expanded --precision=6 --importer=node_modules/node-sass-package-importer/dist/cli.js ./projects/ngx-multiple-dates/src/prebuilt-themes/indigo-pink.scss -o ./dist/ngx-multiple-dates/prebuilt-themes/",
    "css:compile:pink-bluegrey": "node-sass --output-style=expanded --precision=6 --importer=node_modules/node-sass-package-importer/dist/cli.js ./projects/ngx-multiple-dates/src/prebuilt-themes/pink-bluegrey.scss -o ./dist/ngx-multiple-dates/prebuilt-themes/",
    "css:compile:purple-green": "node-sass --output-style=expanded --precision=6 --importer=node_modules/node-sass-package-importer/dist/cli.js ./projects/ngx-multiple-dates/src/prebuilt-themes/purple-green.scss -o ./dist/ngx-multiple-dates/prebuilt-themes/",
    "css:minify": "run-p css:minify:deeppurple-amber css:minify:indigo-pink css:minify:pink-bluegrey css:minify:purple-green",
    "css:minify:deeppurple-amber": "cleancss -O=1 --format=breakWith=lf ./dist/ngx-multiple-dates/prebuilt-themes/deeppurple-amber.css -o ./dist/ngx-multiple-dates/prebuilt-themes/deeppurple-amber.css",
    "css:minify:indigo-pink": "cleancss -O=1 --format=breakWith=lf ./dist/ngx-multiple-dates/prebuilt-themes/indigo-pink.css -o ./dist/ngx-multiple-dates/prebuilt-themes/indigo-pink.css",
    "css:minify:pink-bluegrey": "cleancss -O=1 --format=breakWith=lf ./dist/ngx-multiple-dates/prebuilt-themes/pink-bluegrey.css -o ./dist/ngx-multiple-dates/prebuilt-themes/pink-bluegrey.css",
    "css:minify:purple-green": "cleancss -O=1 --format=breakWith=lf ./dist/ngx-multiple-dates/prebuilt-themes/purple-green.css -o ./dist/ngx-multiple-dates/prebuilt-themes/purple-green.css",
    "css:prefix": "postcss --replace \"./dist/ngx-multiple-dates/prebuilt-themes/*.css\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": false,
  "keywords": [
    "angular",
    "material",
    "date",
    "multiple",
    "datepicker"
  ],
  "dependencies": {
    "@angular/animations": "11.2.4",
    "@angular/cdk": "11.2.3",
    "@angular/common": "11.2.4",
    "@angular/compiler": "11.2.4",
    "@angular/core": "11.2.4",
    "@angular/forms": "11.2.4",
    "@angular/material": "11.2.3",
    "@angular/platform-browser": "11.2.4",
    "@angular/platform-browser-dynamic": "11.2.4",
    "@angular/router": "11.2.4",
    "@material-extended/mde": "3.0.3",
    "hammerjs": "2.0.8",
    "rxjs": "6.6.6",
    "tslib": "2.1.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.3",
    "@angular-devkit/build-ng-packagr": "0.1002.0",
    "@angular/cli": "11.2.3",
    "@angular/compiler-cli": "11.2.4",
    "@angular/language-service": "11.2.4",
    "@types/jasmine": "3.6.4",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "14.14.31",
    "clean-css-cli": "5.2.1",
    "codelyzer": "6.0.1",
    "cpx": "1.5.0",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "6.1.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "ng-packagr": "11.2.4",
    "node-sass": "5.0.0",
    "node-sass-package-importer": "5.3.2",
    "npm-run-all": "4.1.5",
    "postcss-cli": "8.3.1",
    "postcss-load-config": "3.0.1",
    "protractor": "7.0.0",
    "scss-bundle": "3.1.2",
    "ts-node": "9.1.1",
    "tslint": "6.0.0",
    "typescript": "4.1.5"
  }
}
